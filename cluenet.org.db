; vim: ft=bindzone:ts=8:sw=8:nowrap:noet:
$ORIGIN cluenet.org.
$TTL 30m

@		SOA	virgule hostmaster (
				132	; serial
				1h	; refresh
				15m	; retry
				14d	; expire
				5m	; minttl
			)
		NS	ns2 ; virgule.cluenet.org
		NS	ns4 ; wolke.nullroute.eu.org
		NS	ns5 ; sky.nullroute.eu.org -- out-of-date again...

		MX	10 ASPMX.L.GOOGLE.COM.
		MX	20 ALT1.ASPMX.L.GOOGLE.COM.
		MX	20 ALT2.ASPMX.L.GOOGLE.COM.
		MX	30 ASPMX2.GOOGLEMAIL.COM.
		MX	30 ASPMX3.GOOGLEMAIL.COM.
		MX	30 ASPMX4.GOOGLEMAIL.COM.
		MX	30 ASPMX5.GOOGLEMAIL.COM.
		TXT	"v=spf1 a mx ptr ~all"
		SPF	"v=spf1 a mx ptr ~all"

